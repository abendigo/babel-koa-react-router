{
  "name": "routes",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "babel": {
    "blacklist": [
      "regenerator"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",

    "build:vendor.js": "browserify -r react -r react-dom -r react-router -r history -o dist/vendor.js",
    "build:bundle.js": "browserify --no-bundle-external -d -e src/browser.js -t [ babelify --sourceMapRelative . ] -o dist/bundle.js",
    "build:compile": "babel src --out-dir dist",

    "prebuild": "npm run clean",
    "build": "npm run build:compile && npm run build:vendor.js && npm run build:bundle.js",

    "clean": "rimraf dist",

    "start": "node dist/server.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^5.8.23",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "history": "^1.12.5",
    "rimraf": "^2.4.3"
  },
  "dependencies": {
    "koa": "^1.1.0",
    "koa-static": "^1.5.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^1.0.0-rc3"
  }
}
